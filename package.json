{
  "name": "jest-runner-remark",
  "version": "0.0.0-development",
  "description": "Jest runner to check markdown files with remark",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keplersj/jest-runner-remark.git"
  },
  "keywords": [
    "jest",
    "jest-runner",
    "lint",
    "remark",
    "remark-lint",
    "markdown",
    "md"
  ],
  "author": "Kepler Sticka-Jones <kepler@stickajones.org>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/keplersj/jest-runner-remark/issues"
  },
  "homepage": "https://github.com/keplersj/jest-runner-remark#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "create-jest-runner": "^0.7.0",
    "remark": "^14.0.0",
    "unified-engine": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "eslint": "7.32.0",
    "eslint-config-starstuff": "1.4.42",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "jest-runner-eslint": "0.10.1",
    "jest-runner-prettier": "0.3.6",
    "prettier": "2.3.2",
    "semantic-release": "17.4.4"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "eslintConfig": {
    "extends": "starstuff/auto",
    "env": {
      "node": true
    }
  }
}
